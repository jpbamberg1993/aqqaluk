!!!
%html.no-js{:lang => "en"}
  = head

  %body{'data-authenticity_token' => form_authenticity_token}
    .wrapper
      = browsehappy
      %header{:role => "banner"}
        .container
          = link_to '', root_path, :title => 'Aqqaluk Trust', :class => 'logo'
          = yield :main_navigation
          = render :partial => 'layouts/utility'
      #main{:role => "main"}
        = yield
      = render :partial => 'layouts/footer'
    = scripts
